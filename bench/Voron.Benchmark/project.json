    "Microsoft.CSharp": "4.3.0",
    "Microsoft.NETCore.App": {
      "type": "build",
      "version": "1.1.0"
    },
    "Voron": "4.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
      },
      "imports": "dnxcore50"
    }
  },

  "runtimes": {
    "win7-x64": {},
    "win8-x64": {},
    "win10-x64": {},
    "ubuntu.14.04-x64": {}
  },

  "version": "4.0.0-*"
