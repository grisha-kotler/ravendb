{
  "buildOptions": {
    "allowUnsafe": true,
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "debugType": "portable",
    "compile": {
      "include": [ "**/*.cs" ]
    }
  },
  "commands": {
    "Voron.Benchmark": "Voron.Benchmark"
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
